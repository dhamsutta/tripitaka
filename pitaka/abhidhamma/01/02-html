<style>
  /* Wiki Style for Blogger Post */
  .wiki-article-content {
    font-family: 'Bai Jamjuree', Arial, sans-serif;
    font-size: 20px; /* User preference */
    line-height: 1.7; /* User preference */
    color: #202122; /* Wikipedia text color */
    background-color: #ffffff; /* White background */
    text-align: left; /* User preference */
  }
  .wiki-article-content h2 {
    font-size: 1.6em; /* Approx 32px */
    color: #000000;
    border-bottom: 1px solid #a2a9b1; /* Wikipedia heading border */
    padding-bottom: 0.3em;
    margin-top: 25px;
    margin-bottom: 16px; /* User preference */
    font-weight: normal; /* Wikipedia titles are often not overly bold */
  }
  .wiki-article-content p,
  .wiki-article-content ul {
    margin-bottom: 16px; /* User preference */
    text-align: left; /* User preference */
  }
  .wiki-article-content ul {
    padding-left: 1.8em; /* Indent lists */
    list-style-type: disc; /* Standard disc for ul */
  }
  .wiki-article-content li {
    margin-bottom: 8px;
  }
  .wiki-article-content a {
    color: #0645ad; /* Wikipedia blue */
    text-decoration: underline;
  }
  .wiki-article-content a:visited {
    color: #0b0080; /* Wikipedia purple for visited links */
  }
  .wiki-article-content strong { /* For existing strong tags, or if used without tooltip span */
    font-weight: bold;
  }
  .wiki-article-content .tooltip-term strong { /* Ensure strong inside span is bold */
      font-weight: bold;
  }
  /* Tooltip span itself doesn't need much styling, browser handles title attribute */
  .tooltip-term {
    /* border-bottom: 1px dotted #54595d; /* Optional: subtle underline for tooltip terms */
    /* cursor: help; */
  }
  .wiki-article-content hr {
    margin-top: 20px;
    margin-bottom: 20px;
    border: 0;
    border-top: 1px solid #c8ccd1; /* Wikipedia hr color */
  }
  .wiki-article-content .section-footer { /* For tags and links sections */
    margin-top:30px;
    padding-top:15px;
    border-top: 1px solid #c8ccd1;
  }
</style>

